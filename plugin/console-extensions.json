[
 {
    "type": "console.flag/hookProvider",
    "properties": {
      "handler": { "$codeRef": "quarkusActions.enableQuarkusPlugin" }
    }
  },
  {
    "type": "console.page/route",
    "properties": {
      "exact": true,
      "path": "/quarkus",
      "component": { "$codeRef": "QuarkusHomePage" }
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
      "id": "quarkusHome",
      "name": "%quarkus-plugin~Quarkus%",
      "href": "/quarkus",
      "perspective": "dev",
      "section": "resources"
    }
  }
]
